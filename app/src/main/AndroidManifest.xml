<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.miracosta.cs134.mkatsaros.mccconnect">

    <application
        android:allowBackup="true"
        android:icon="@drawable/spartan"
        android:label="@string/app_name"
        android:roundIcon="@drawable/spartan_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ReviewViewActivity"></activity>
        <activity android:name=".ReviewSuccessActivity" />
        <activity android:name=".WriteReviewActivity" />
        <activity android:name=".CreateActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".ReviewListAdapter" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
    </application>

</manifest>